{
  "name": "bondarys-backend",
  "version": "1.0.0",
  "description": "Bondarys Family Management App Backend API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "node src/scripts/run-migrations.js migrate",
    "migrate:status": "node src/scripts/run-migrations.js status",
    "migrate:ts": "ts-node src/scripts/migrate.ts migrate",
    "migrate:rollback": "ts-node src/scripts/migrate.ts rollback",
    "migrate:create": "ts-node src/scripts/migrate.ts create",
    "migrate:validate": "ts-node src/scripts/migrate.ts validate",
    "migrate:reset": "ts-node src/scripts/migrate.ts reset --confirm",
    "db:health": "node src/scripts/health-check.js",
    "db:stats": "node src/scripts/db-stats.js",
    "db:cleanup": "ts-node -e \"import('./src/services/migrationService.js').then(m => m.migrationService.cleanup_old_data())\"",
    "docker:build": "docker build -t bondarys-backend .",
    "docker:run": "docker run -p 3000:3000 bondarys-backend"
  },
  "keywords": [
    "family",
    "management",
    "api",
    "nodejs",
    "express",
    "supabase",
    "postgresql",
    "socketio",
    "webrtc"
  ],
  "author": "Bondarys Team",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^9.43.0",
    "@sentry/profiling-node": "^1.0.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "@supabase/supabase-js": "^2.38.0",
    "@types/multer": "^2.0.0",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1438.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.0.0",
    "google-auth-library": "^8.9.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "pg": "^8.16.3",
    "redis": "^4.6.8",
    "sharp": "^0.32.5",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.7.2",
    "stripe": "^13.5.0",
    "twilio": "^4.12.0",
    "uuid": "^9.0.0",
    "web-push": "^3.6.7",
    "webrtc-adapter": "^8.2.3",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/xlsx": "^0.0.36",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bondarys/backend.git"
  },
  "bugs": {
    "url": "https://github.com/bondarys/backend/issues"
  },
  "homepage": "https://github.com/bondarys/backend#readme"
}
