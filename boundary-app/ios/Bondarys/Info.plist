<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>Bondarys</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0.0</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>api.bondarys.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
				<key>NSExceptionRequiresForwardSecrecy</key>
				<true/>
				<key>NSIncludesSubdomains</key>
				<true/>
			</dict>
		</dict>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- Permissions -->
	<key>NSCameraUsageDescription</key>
	<string>Bondarys needs camera access to take family photos and scan QR codes for family invitations.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Bondarys needs photo library access to share family photos and save important images.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Bondarys needs location access to show family member locations and provide safety features.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Bondarys needs location access to provide real-time family safety features and emergency alerts.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Bondarys needs background location access to provide continuous family safety monitoring.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Bondarys needs microphone access for voice calls and voice messages with family members.</string>
	<key>NSCalendarsUsageDescription</key>
	<string>Bondarys needs calendar access to sync family events and activities.</string>
	<key>NSContactsUsageDescription</key>
	<string>Bondarys needs contacts access to add family members and emergency contacts.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>Bondarys uses Face ID to securely log you into your family account.</string>
	<key>NSMotionUsageDescription</key>
	<string>Bondarys uses motion data to detect emergency situations and provide automatic safety alerts.</string>
	<key>NSBluetoothAlwaysUsageDescription</key>
	<string>Bondarys uses Bluetooth to connect with family devices for enhanced safety features.</string>
	<key>NSBluetoothPeripheralUsageDescription</key>
	<string>Bondarys uses Bluetooth to connect with family devices for enhanced safety features.</string>
	
	<!-- Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
		<string>background-processing</string>
		<string>background-fetch</string>
		<string>remote-notification</string>
		<string>voip</string>
		<string>audio</string>
	</array>
	
	<!-- URL Schemes -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.bondarys.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>bondarys</string>
				<string>bondarys-app</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.bondarys.auth</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.bondarys.auth</string>
			</array>
		</dict>
	</array>
	
	<!-- Associated Domains -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>applinks:bondarys.com</string>
		<string>webcredentials:bondarys.com</string>
	</array>
	
	<!-- Supported Languages -->
	<key>CFBundleLocalizations</key>
	<array>
		<string>en</string>
		<string>th</string>
		<string>zh-Hans</string>
		<string>zh-Hant</string>
		<string>ja</string>
		<string>ko</string>
	</array>
	
	<!-- App Icons -->
	<key>CFBundleIcons</key>
	<dict>
		<key>CFBundlePrimaryIcon</key>
		<dict>
			<key>CFBundleIconFiles</key>
			<array>
				<string>AppIcon</string>
			</array>
			<key>CFBundleIconName</key>
			<string>AppIcon</string>
		</dict>
	</dict>
	
	<!-- App Store Information -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>tel</string>
		<string>sms</string>
		<string>mailto</string>
		<string>facetime</string>
		<string>facetime-audio</string>
	</array>
	
